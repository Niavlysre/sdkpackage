
<component
	id="FioriButtonM"
	title="Fiori Button 2.0"
	tooltip="Fiori Button 2.0"
	icon="res/FioriButtonM/FioriButtonM.png"
	handlerType="sapui5"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
	modes="m"
>
	
	<requireJs modes="m">res/FioriButtonM/FioriButtonM</requireJs>

	<cssInclude>res/FioriButtonM/FioriButtonM.css</cssInclude>
	
	
	<property
		id="dBackColor"
		type="String"
		title="Background Colour"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="dFontColor"
		type="String"
		title="Font Colour"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="dIcon"
		type="String"
		title="Icon"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="dSize"
		type="String"
		title="Size M (16px) | L (22px)"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="dStyle"
		type="String"
		title="Button Style"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="dText"
		type="String"
		title="Text"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="dTooltip"
		type="String"
		title="Tooltip"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onClick"
		type="ScriptText"
		title="Event For On Click"
		group="Events"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">100</defaultValue>
		<defaultValue property="HEIGHT">42</defaultValue>
		
		<defaultValue property="dBackColor"><![CDATA[]]></defaultValue>
		<defaultValue property="dFontColor"><![CDATA[]]></defaultValue>
		<defaultValue property="dIcon"><![CDATA[sap-icon://home]]></defaultValue>
		<defaultValue property="dSize"><![CDATA[L]]></defaultValue>
		<defaultValue property="dStyle"><![CDATA[Default]]></defaultValue>
		<defaultValue property="dText"><![CDATA[]]></defaultValue>
		<defaultValue property="dTooltip"><![CDATA[]]></defaultValue>
		<defaultValue property="onClick"><![CDATA[]]></defaultValue>

	</initialization>

</component>
