  xmlns:mvc="sap.ui.core.mvc"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"  
  xmlns:s="sap.suite.ui.commons"
  xmlns:ld="PropertyPage.LumiraDesigner"
  controllerName="PropertyPage.General" >
	<Page title="Main Properties" content="{path : 'navState>/tabs', factory : '.fragmentFactory'}">
